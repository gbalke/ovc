/include/ "system-conf.dtsi"
/ {
	model = "Enclustra ME-XU5-4EV-1I-D11E SOM";

  reserved-memory {
    #address-cells = <2>;
    #size-cells = <2>;
    ranges;

    reserved: framebuffer@0x70000000 {
       no-map;
       reg = <0x0 0x70000000 0x0 0x10000000>;
    };
  };

  reserved-driver@0 {
    compatible = "xlnx,reserved-memory";
    memory-region = <&reserved>;
  };

  // Video pipeline
  /*
  axi_video_cap {
      compatible = "xlnx,video";
      dmas = <&axi_vdma_2 1>;
      dma-names = "port0";
    
      ports {
          #address-cells = <1>;
          #size-cells = <0>;
    
          port@0 {
              reg = <0>;
              direction = "input";
              vcap0_in0: endpoint {
                  remote-endpoint = <&mipi_csi2_rx_subsyst_0>;
              };
          };
      };
  };
  */


  // I2C mux
  i2cmux {
    compatible = "i2c-mux-gpio";
    #address-cells = <1>;
    #size-cells = <0>;
    mux-gpios = <&gpio 78 0>; // pin in index 0
    i2c-parent = <&i2c1>;

    // Cam 0
    i2c@0 {
      #address-cells = <1>;
      #size-cells = <0>;
			//compatible = "cdns,i2c-r1p14\0cdns,i2c-r1p10";
			//status = "okay";
      reg = <0>;
    };

    // Cam 1
    i2c@1 {
      #address-cells = <1>;
      #size-cells = <0>;
      reg = <1>;
    };

    // Cam 2
    i2c@2 {
      #address-cells = <1>;
      #size-cells = <0>;
      reg = <2>;
    };

    // Cam 3
    i2c@3 {
      #address-cells = <1>;
      #size-cells = <0>;
      reg = <3>;
    };

    // Cam 4
    i2c@4 {
      #address-cells = <1>;
      #size-cells = <0>;
      reg = <4>;
    };

    // Cam 5
    i2c@5 {
      #address-cells = <1>;
      #size-cells = <0>;
      reg = <5>;
    };

  };

  udmabuf@0 {
    compatible = "ikwzm,u-dma-buf";
    device-name = "cam0_0";
    size = <0x1000000>; // 16 MB
    sync-size = <0x1000000>; // 16 MB
    sync-direction = <2>; // From device
  };

